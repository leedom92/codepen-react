{
  "name": "codepen-react",
  "version": "1.0.1",
  "description": "A react component to embed codepen",
  "packageManager": "pnpm@9.10.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "release": "pnpm build && bumpp && pnpm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leedom92/codepen-react.git"
  },
  "keywords": [
    "react",
    "react-component",
    "codepen",
    "embed"
  ],
  "author": "Leedom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leedom92/codepen-react/issues"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/leedom92/codepen-react#readme",
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-react-jsx": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "bumpp": "^9.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  }
}
